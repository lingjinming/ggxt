/* pages/trainPlan/trainPlan.wxss */
.container {
  font-size: 24rpx;
  color: #fff;
}

.content{
  margin-bottom: 0;
}
.result-head {
  padding-left: 40rpx;
}

.inner-table-head {
  height: auto;
  background: #29c6b2;
  padding-left: 40rpx;
  padding-top:15rpx;
padding-bottom:15rpx;

}

.inner-table-head > text, .inner-table-head view {
  line-height: 22px;
  color: #fff;
}

.infowrap text {
  margin-right: 80rpx;
  font-size: 26rpx;
}

.plantitle {
  text-align: center;
}

.flex-wrap {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #94e3d9;
  line-height: 2;
  margin-bottom: 20rpx;
}

picker {
  height: 100%;
}

.table-wrap {
  position: relative;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 40rpx;
  margin-bottom: 40rpx;
  padding-bottom: 0;
}

.inner-table {
  color: #595757;
}

.inner-table-body {
  justify-content: space-around;
  border: none;
  margin-bottom: 0;
  position: relative;
}

.inner-table-body text {
  text-align: center;
  flex: 1;
  border: 1px solid #ccc;
  border-top: none;
}
.inner-table-body text:nth-child(2){
  border-right: none;
    flex: 2;

}

/* .inner-table-body text:nth-child(3) {
    border-right: none;
} */
.textalign-c text {
color:#595757;
font-size:30rpx;
margin-bottom:10rpx;
display:block;
margin-top:10rpx;

}

scroll-view {
  height: 920rpx;
}

.result-head{
  height: auto;
}
.inner-table-body text{
display:flex;
align-items:center;
justify-content:center;

}
.inner-table-body:nth-child(3) text:nth-child(4),
.inner-table-body:nth-child(4) text:nth-child(4),
.inner-table-body:nth-child(5) text:nth-child(4){
  color: transparent;
  border-top: none;
  border-bottom: none;
}
.inner-table-body:nth-child(4) text:nth-child(4){
  color: inherit;
}
.inner-table-body:nth-child(6) text:nth-child(3){
  border-bottom: 1px solid #ccc;
}
image{
  height: 30rpx;
  width: 30rpx;
  position: absolute;
  left:25rpx;
  top: 10rpx
}
.inner-table-body:nth-child(6) text:last-child{
  color: transparent;
}
.inner-table-body text:last-child{
  border-left: none;
}
.block{
  display: block
}
.none{
  display: none
}
.video_pay{
  position: absolute;
  /* background: red; */
  height: 600rpx;
  width: 600rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  color: #fff;
  opacity: .99;
}
.video_pay_con_tit{
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 20rpx;
  text-align: center;
  font-size: 40rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
